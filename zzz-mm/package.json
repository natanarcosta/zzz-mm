{
  "name": "zzz-mm",
  "main": "main.js",
  "version": "0.1.1",
  "author": "natanarcosta",
  "description": "Ava's ZZZ Mod Manager",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:zzz-mm": "node dist/zzz-mm/server/server.mjs",
    "electron:dev": "concurrently \"webpack --watch\" \"ng serve\" \"electron .\"",
    "electron:dist": "rimraf dist && cross-env NODE_ENV=production webpack && ng build --configuration production && electron-builder"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
